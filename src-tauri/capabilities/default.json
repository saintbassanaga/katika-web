{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        { "url": "http://localhost:8080/**" },
        { "url": "https://api.katika.cm/**" }
      ]
    },
    "shell:allow-open",
    "fs:default",
    "notification:default"
  ]
}

